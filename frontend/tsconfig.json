{
	"compilerOptions": {
		"noEmit": true,
		"target": "es5",
		"module": "es6",
		"jsx": "react",
		"noImplicitAny": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/*"],
			"@components/*": ["./src/components/*"],
			"@assets/*": ["./src/assets/*"],
			"@styles/*": ["./src/styles/*"],
			"@types/*": ["./src/types/*"]
		}
	}
}
